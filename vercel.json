{
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "https://portfoilo-bay.vercel.app" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
        ]
      }
    ],
    "functions": {
      "api/contact.js": {
        "runtime": "nodejs18.x",
        "includeFiles": ["node_modules/**"]
      }
    }
  }
  